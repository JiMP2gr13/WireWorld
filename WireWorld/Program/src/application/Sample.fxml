		<Canvas height="335.0" layoutX="214.0" layoutY="38.0" width="400.0"
			fx:id="canvas" />
		<Button layoutX="56.0" layoutY="93.0" mnemonicParsing="false"
			prefHeight="25.0" prefWidth="145.0" text="Wybierz plik wejściowy" onAction="#handleFileChooseButton"/>
